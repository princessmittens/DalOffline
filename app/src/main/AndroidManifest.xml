<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.achristians.gpproject">
    <!-- <application android:name="com.example.achristians.gpproject.firebase"></application> -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DalOffline"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="DalOffline">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Menu" android:label="Choose an Option" android:exported="true"> </activity>
        <activity android:name=".SignUpPage" android:label="Sign Up"> </activity>
        <activity android:name=".CourseList" android:label="Course List"
            android:parentActivityName=".Menu"> </activity>
        <activity android:name=".CourseDetails" android:parentActivityName=".Menu"
            android:label="Course Details" />
        <activity android:name=".WeeklySchedule" android:parentActivityName=".Menu" android:label="View Your Timetable"></activity>
        <activity android:name=".MyCourses" android:label="My Courses"></activity>
    </application>

</manifest>
